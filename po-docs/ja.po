#: ../examples/guestfs-recipes.pod:193
#, no-wrap
msgid ""
" #!/bin/sh -\n"
" set -e\n"
" eval \"$(guestfish --ro -d \"$1\" --i --listen)\"\n"
" root=\"$(guestfish --remote inspect-get-roots)\"\n"
" guestfish --remote inspect-get-product-name \"$root\"\n"
