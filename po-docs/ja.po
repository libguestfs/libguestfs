msgstr ""

# type: textblock
#. type: textblock
#: ../src/guestfs-actions.pod:6713
msgid "(Added in 1.0.41)"
msgstr ""

# type: =head2
#. type: =head2
#: ../src/guestfs-actions.pod:6715 ../src/guestfs-structs.pod:109
msgid "guestfs_stat"
msgstr ""

