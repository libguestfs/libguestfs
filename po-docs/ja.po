msgstr "blockdev-getro"

#. type: verbatim
#: ../fish/guestfish-actions.pod:555
#, no-wrap
msgid ""
" blockdev-getro device\n"
"\n"
msgstr ""
" blockdev-getro device\n"
"\n"

#. type: textblock
