#: ../examples/guestfs-testing.pod:159
#, no-wrap
msgid ""
" virt-filesystems -a /path/to/disk.img --all --long -h\n"
"\n"
msgstr ""
" virt-filesystems -a /path/to/disk.img --all --long -h\n"
"\n"

#. type: verbatim
#: ../examples/guestfs-testing.pod:163
#, no-wrap
msgid ""
" virt-filesystems -d Guest --all --long -h\n"
"\n"
msgstr ""
" virt-filesystems -d Guest --all --long -h\n"
"\n"

