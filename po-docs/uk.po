#. type: =head2
#: ../src/guestfs-actions.pod:7182
msgid "guestfs_wc_c"
msgstr ""

#. type: verbatim
#: ../src/guestfs-actions.pod:7184
#, no-wrap
msgid ""
" int\n"
" guestfs_wc_c (guestfs_h *g,\n"
"               const char *path);\n"
"\n"
msgstr ""

#. type: textblock
#: ../src/guestfs-actions.pod:7188 ../fish/guestfish-actions.pod:4810
