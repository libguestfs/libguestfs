msgstr ""

#. type: =item
#: ../fuse/guestmount.pod:233
msgid "B<-V> | B<--version>"
msgstr ""

#. type: textblock
#: ../fuse/guestmount.pod:235
msgid "Display the program version and exit."
msgstr ""

#. type: =item
#: ../fuse/guestmount.pod:237
msgid "B<-w> | B<--rw>"
msgstr ""

#. type: textblock
#: ../fuse/guestmount.pod:242 ../fuse/guestmount.pod:263
