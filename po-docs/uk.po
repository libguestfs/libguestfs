msgstr "/etc/libguestfs-tools.conf"

#. type: textblock
#: ../fish/guestfish.pod:1547 ../fuse/guestmount.pod:411
#: ../rescue/virt-rescue.pod:395
msgid ""
"This configuration file controls the default read-only or read-write mode "
"(I<--ro> or I<--rw>)."
msgstr ""

#. type: textblock
#: ../fish/guestfish.pod:1550 ../fuse/guestmount.pod:414
#: ../rescue/virt-rescue.pod:398
msgid "See L<libguestfs-tools.conf(5)>."
msgstr "Див. L<libguestfs-tools.conf(5)>."

#. type: =item
#: ../fish/guestfish.pod:1552
msgid "$HOME/.guestfish"
msgstr "$HOME/.guestfish"

#. type: textblock
#: ../fish/guestfish.pod:1554
msgid ""
"If compiled with GNU readline support, then the command history is saved in "
"this file."
msgstr ""

#. type: =item
#: ../fish/guestfish.pod:1557
