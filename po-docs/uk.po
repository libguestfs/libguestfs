#. type: verbatim
#: ../p2v/virt-p2v-make-disk.pod:77 ../p2v/virt-p2v-make-kickstart.pod:221
#, no-wrap
msgid ""
" │         User name: [root_____________________________] │\n"
" │                                                        │\n"
" │          Password: [    <leave this field blank>     ] │\n"
" │                                                        │\n"
" │  SSH Identity URL: [file:///var/tmp/id_rsa___________] │\n"
"\n"
msgstr ""

#. type: textblock
#: ../p2v/virt-p2v-make-disk.pod:83 ../p2v/virt-p2v-make-kickstart.pod:227
msgid "or if using the kernel command line, add:"
msgstr ""

#. type: verbatim
#: ../p2v/virt-p2v-make-disk.pod:85 ../p2v/virt-p2v-make-kickstart.pod:229
#: ../p2v/virt-p2v.pod:530
#, no-wrap
msgid ""
" p2v.identity=file:///var/tmp/id_rsa\n"
"\n"
msgstr ""

#. type: textblock
#: ../p2v/virt-p2v-make-disk.pod:87 ../p2v/virt-p2v-make-kickstart.pod:231
