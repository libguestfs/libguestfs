#: ../src/guestfs-actions.pod:6355 ../src/guestfs-actions.pod:6381
#: ../src/guestfs-actions.pod:6512 ../src/guestfs-actions.pod:6538
#: ../src/guestfs-actions.pod:6562 ../src/guestfs-actions.pod:7517
#: ../src/guestfs-actions.pod:7572 ../src/guestfs-actions.pod:7718
#: ../src/guestfs-actions.pod:7742 ../src/guestfs-actions.pod:8529
#: ../src/guestfs-actions.pod:8549 ../src/guestfs-actions.pod:8582
#: ../src/guestfs-actions.pod:8601 ../src/guestfs-actions.pod:8620
#: ../src/guestfs-actions.pod:8711 ../src/guestfs-actions.pod:8730
