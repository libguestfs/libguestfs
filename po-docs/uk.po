#, no-wrap
msgid ""
" more filename\n"
"\n"
msgstr ""

#. type: verbatim
#: ../fish/guestfish-commands.pod:204
#, no-wrap
msgid ""
" less filename\n"
"\n"
msgstr ""

#. type: textblock
#: ../fish/guestfish-commands.pod:206
