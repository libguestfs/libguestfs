msgstr ""

#. type: =item
#: ../src/guestfs-actions.pod:1572 ../fish/guestfish-actions.pod:1044
msgid "\"sparc\""
msgstr ""

#. type: textblock
#: ../src/guestfs-actions.pod:1574 ../fish/guestfish-actions.pod:1046
msgid "32 bit SPARC."
msgstr ""

#. type: =item
#: ../src/guestfs-actions.pod:1576 ../fish/guestfish-actions.pod:1048
