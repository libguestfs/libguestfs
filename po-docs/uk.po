#. type: =item
#: ../builder/virt-builder.pod:356 ../dib/virt-dib.pod:209
#: ../get-kernel/virt-get-kernel.pod:76 ../resize/virt-resize.pod:445
#: ../sparsify/virt-sparsify.pod:223 ../v2v/virt-v2v.pod:363
msgid "B<--machine-readable>"
msgstr "B<--machine-readable>"

#. type: textblock
#: ../builder/virt-builder.pod:358 ../dib/virt-dib.pod:211
#: ../get-kernel/virt-get-kernel.pod:78 ../resize/virt-resize.pod:447
#: ../sparsify/virt-sparsify.pod:225 ../v2v/virt-v2v.pod:365
msgid ""
"This option is used to make the output more machine friendly when being "
"parsed by other programs.  See L</MACHINE READABLE OUTPUT> below."
msgstr ""

#. type: =item
#: ../builder/virt-builder.pod:362 ../customize/virt-customize.pod:125
#: ../dib/virt-dib.pod:215 ../utils/boot-analysis/boot-analysis.pod:70
#: ../utils/boot-benchmark/boot-benchmark.pod:45
msgid "B<-m> MB"
msgstr ""

#. type: =item
#: ../builder/virt-builder.pod:364 ../customize/virt-customize.pod:127
#: ../dib/virt-dib.pod:217 ../utils/boot-analysis/boot-analysis.pod:72
#: ../utils/boot-benchmark/boot-benchmark.pod:47
msgid "B<--memsize> MB"
msgstr ""

#. type: textblock
#: ../builder/virt-builder.pod:366 ../customize/virt-customize.pod:129
msgid ""
"Change the amount of memory allocated to I<--run> scripts.  Increase this if "
"you find that I<--run> scripts or the I<--install> option are running out of "
"memory."
msgstr ""

#. type: textblock
#: ../builder/virt-builder.pod:370 ../customize/virt-customize.pod:133
#: ../dib/virt-dib.pod:222
msgid "The default can be found with this command:"
msgstr ""

#. type: verbatim
#: ../builder/virt-builder.pod:372 ../customize/virt-customize.pod:135
#: ../dib/virt-dib.pod:224
#, no-wrap
msgid ""
" guestfish get-memsize\n"
"\n"
msgstr ""

#. type: =item
#: ../builder/virt-builder.pod:374 ../customize/virt-customize.pod:137
#: ../daemon/guestfsd.pod:77 ../dib/virt-dib.pod:237 ../fish/guestfish.pod:334
#: ../rescue/virt-rescue.pod:182 ../sysprep/virt-sysprep.pod:222
