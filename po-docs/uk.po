msgstr "$XDG_CONFIG_HOME/libguestfs/libguestfs-tools.conf"

#. type: =item
#: ../fish/guestfish.pod:1541 ../fish/libguestfs-tools.conf.pod:67
#: ../fuse/guestmount.pod:405 ../rescue/virt-rescue.pod:389
