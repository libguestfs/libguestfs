#: ../fish/guestfish-commands.pod:190
msgid "man"
msgstr "man"

#. type: =head2
#: ../fish/guestfish-commands.pod:192
msgid "manual"
msgstr "підручник"

#. type: verbatim
#: ../fish/guestfish-commands.pod:194
#, no-wrap
msgid ""
"  man\n"
"\n"
msgstr ""
"  man\n"
"\n"

