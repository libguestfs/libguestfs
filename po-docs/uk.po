" #!/bin/sh -\n"
" \n"

#. type: verbatim
#: ../docs/guestfs-recipes.pod:363 ../fish/guestfish.pod:1008
#, no-wrap
msgid ""
" set -e\n"
" \n"
msgstr ""
" set -e\n"
" \n"

#. type: verbatim
#: ../docs/guestfs-recipes.pod:365
#, no-wrap
msgid ""
" vm=\"$1\"\n"
" dir=/home\n"
" \n"
msgstr ""
" vm=\"$1\"\n"
" dir=/home\n"
" \n"

#. type: verbatim
#: ../docs/guestfs-recipes.pod:368
#, no-wrap
msgid ""
" eval $(guestfish --ro -d \"$vm\" -i --listen)\n"
" \n"
msgstr ""
