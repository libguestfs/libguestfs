" \n"

#. type: verbatim
#: ../docs/guestfs-recipes.pod:370
#, no-wrap
msgid ""
" for d in $(guestfish --remote ls \"$dir\"); do\n"
"     echo -n \"$dir/$d\"\n"
"     echo -ne '\\t'\n"
"     guestfish --remote du \"$dir/$d\";\n"
" done | sort -nr -k 2\n"
" \n"
msgstr ""
" for d in $(guestfish --remote ls \"$dir\"); do\n"
"     echo -n \"$dir/$d\"\n"
"     echo -ne '\\t'\n"
"     guestfish --remote du \"$dir/$d\";\n"
" done | sort -nr -k 2\n"
" \n"

#. type: verbatim
#: ../docs/guestfs-recipes.pod:376 ../fish/guestfish.pod:962
#, no-wrap
msgid ""
" guestfish --remote exit\n"
"\n"
msgstr ""
" guestfish --remote exit\n"
"\n"

