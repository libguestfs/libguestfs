msgstr ""

#. type: =head2
#: ../fish/guestfish-actions.pod:2951
msgid "hivex-node-children"
msgstr "hivex-node-children"

#. type: verbatim
#: ../fish/guestfish-actions.pod:2953
#, no-wrap
msgid ""
" hivex-node-children nodeh\n"
"\n"
msgstr ""

