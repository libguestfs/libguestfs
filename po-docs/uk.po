msgstr "llz"

#. type: verbatim
#: ../fish/guestfish-actions.pod:4562
#, no-wrap
msgid ""
" llz directory\n"
"\n"
msgstr ""
" llz каталог\n"
"\n"

#. type: textblock
#: ../fish/guestfish-actions.pod:4564 ../src/guestfs-actions.pod:7266
